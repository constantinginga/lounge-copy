@page "/my-profile"
@inject NavigationManager NavManager
@inject AppState AppState

<div class="wrapper">
<iframe src=@("http://localhost:5173/?memberId=" + AppState.CurrentUser.Id) frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
</div>

@code {
    protected override async Task OnParametersSetAsync()
    {
        if (!AppState.IsLoggedIn)
        {
            AppState.PreviousPage = NavManager.Uri;
            NavManager.NavigateTo("./login");
            return;
        }
    }
 }