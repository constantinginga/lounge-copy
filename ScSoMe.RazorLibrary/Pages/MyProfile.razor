@page "/my-profile"
@inject NavigationManager NavManager
@inject AppState AppState
@inject Blazored.LocalStorage.ILocalStorageService localStorage

<div class="wrapper" style="min-height:100vh; height:100%">
<iframe src=@("http://localhost:5173/?memberId=" + AppState.CurrentUser.Id + "&token=" + AppState.token) style="min-height:100vh; height:100%" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
</div>

@code {
    protected override async Task OnParametersSetAsync()
    {
        if (!AppState.IsLoggedIn)
        {
            AppState.PreviousPage = NavManager.Uri;
            NavManager.NavigateTo("./login");
            return;
        }
    }
 }